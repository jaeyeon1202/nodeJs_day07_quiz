<%- include("../default/header") %>

<h3>회 원 정 보 페 이 지</h3>

<% console.log("리스트 출력::", list) %>
<table border="1">
    <tr><th>아이디</th> <th>비밀번호</th> <th>이름</th> <th>주소</th></tr>


    <% list.forEach(member=>{ %>
       <tr>
            <td><%= member.ID %></td>
            <td><%= member.PWD %></td>
            <td> <a href="/member/info?id=<%= member.ID%>"><%= member.NAME %></a></td>
            <td><%= member.ADDR %></td>
       </tr> 
    <% }); %>
</table>